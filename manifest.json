{
  "manifest_version": 2,
  "name": "Chrome Wheather Viewer",
  "version": "0.10.0",
  "homepage_url": "http://blog.javarouka.me/",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "icons":{
    "16" : "images/mdn-16.png",
    "24" : "images/mdn-24.png",
    "48" : "images/mdn-48.png",
    "128" : "images/orig.png"
  },
  "browser_action": {
    "default_icon": "images/mdn-24.png",
    "default_popup": "main.html"
  },
  "permissions": [
    "tabs",
    "clipboardRead",
    "http://ajax.aspnetcdn.com/",
    "http://search.daum.net//"
  ]
}